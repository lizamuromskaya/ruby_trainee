<h1>
  Clients
</h1>
<table class="table table-striped">
	<thead>
		<tr>
		<th>Name</th>
		<th>Phone</th>
		<th>Date and time</th>
		<th>Barber</th>
		<th>Color</th>
	</tr>
	</thead>


<tbody>
	<% @results.each do |row| %>
	<tr>
		<td><%= row['username'] %></td>
		<td><%= row['phone'] %></td>
		<td><%= row['datetime'] %></td>
		<td><%= row['barber'] %></td>
		<td><%= row['color'] %></td>
	</tr>


	<% end %>

</tbody>
</table>